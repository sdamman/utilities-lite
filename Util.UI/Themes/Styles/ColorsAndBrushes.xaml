  <Color x:Key="colorUranianBlue80t" A="#CC" R="#AD" G="#DC" B="#FF"/>

  <Color x:Key="colorBlack" A="#FF" R="#00" G="#00" B="#00"/>
  <Color x:Key="colorDark100" A="#FF" R="#20" G="#20" B="#20"/>
  <Color x:Key="colorDark80" A="#FF" R="#40" G="#40" B="#40"/>
